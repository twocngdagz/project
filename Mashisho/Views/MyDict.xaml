<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:my="clr-namespace:Mashisho">
    <DataTemplate DataType="{x:Type my:Tenant}">
        <my:LandlordDetail />
    </DataTemplate>
    <DataTemplate DataType="{x:Type my:PartialTenant}">
        <my:LandlordDetail />
    </DataTemplate>
    <DataTemplate DataType="{x:Type my:TenantFilter}">
        <my:TenantFilterControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type my:Landlord}">
        <my:LandlordDetail />
    </DataTemplate>
    <DataTemplate DataType="{x:Type my:PartialLandlord}">
        <my:LandlordDetail />
    </DataTemplate>
    <DataTemplate DataType="{x:Type my:LandlordFilter}">
        <my:LandlordFilterControl />
    </DataTemplate>
</ResourceDictionary>